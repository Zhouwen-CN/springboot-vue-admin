<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yeeiee.scheduler.mapper.JobMapper">
    <!--@formatter:off-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yeeiee.scheduler.domain.entity.Job">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="handler_name" property="handlerName" />
        <result column="handler_param" property="handlerParam" />
        <result column="cron_expression" property="cronExpression" />
        <result column="retry_count" property="retryCount" />
        <result column="retry_interval" property="retryInterval" />
        <result column="job_enable" property="jobEnable" />
        <result column="create_user" property="createUser" />
        <result column="create_time" property="createTime" />
        <result column="update_user" property="updateUser" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, handler_name, handler_param, cron_expression, retry_count, retry_interval, job_enable, create_user, create_time, update_user, update_time
    </sql>
</mapper>